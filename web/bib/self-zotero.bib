@article{rubin2017,
  title = {When {{Does HARKing Hurt}}? {{Identifying When Different Types}} of {{Undisclosed Post Hoc Hypothesizing Harm Scientific Progress}}},
  shorttitle = {When {{Does HARKing Hurt}}?},
  author = {Rubin, Mark},
  year = {2017},
  month = dec,
  journal = {Review of General Psychology},
  volume = {21},
