@techreport{chiarelli2021,
  title = {The {{Art}} of {{Publishing Reproducible Research Outputs}}: {{Supporting}} Emerging Practices through Cultural and Technological Innovation.},
  shorttitle = {The {{Art}} of {{Publishing Reproducible Research Outputs}}},
  author = {Chiarelli, Andrea and Loffreda, Lucia and Johnson, Rob},
  year = {2021},
