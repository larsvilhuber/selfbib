@article{ozier2020,
  title = {Replication {{Redux}}: {{The Reproducibility Crisis}} and the {{Case}} of {{Deworming}}},
  shorttitle = {Replication {{Redux}}},
  author = {Ozier, Owen},
  year = {2020},
