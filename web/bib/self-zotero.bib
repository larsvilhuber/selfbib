@article{AngristJ.Econ.Lit.2020,
  title = {Inside {{Job}} or {{Deep Impact}}? {{Extramural Citations}} and the {{Influence}} of {{Economic Scholarship}}},
  shorttitle = {Inside {{Job}} or {{Deep Impact}}?},
  author = {Angrist, Josh and Azoulay, Pierre and Ellison, Glenn and Hill, Ryan and Lu, Susan Feng},
  year = {2020},
