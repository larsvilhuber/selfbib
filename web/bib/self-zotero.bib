  title = {Investigating the {{Effectiveness}} of the {{Open Data Badge Policy}} at {{Psychological Science Through Computational Reproducibility}}},
  author = {Cr{\"u}well, Sophia and Apthorp, Deborah and Baker, Bradley James and Colling, Lincoln and Elson, Malte and Geiger, Sandra J. and Lobentanzer, Sebastian and Mon{\'e}ger, Jean and Patterson, Alex and Schwarzkopf, D. Sam and Zaneva, Mirela and Brown, Nicholas J. L.},
  year = {2022},
