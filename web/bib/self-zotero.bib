@article{CoffmanJ.Econ.Perspect.2015,
  title = {Pre-Analysis {{Plans Have Limited Upside}}, {{Especially Where Replications Are Feasible}}},
  author = {Coffman, Lucas C. and Niederle, Muriel},
  year = {2015},
  month = sep,
  journal = {Journal of Economic Perspectives},
  volume = {29},
  number = {3},
  pages = {81--98},
  issn = {0895-3309},
  doi = {10.1257/jep.29.3.81},
  url = {https://www.aeaweb.org/articles?id=10.1257/jep.29.3.81},
  urldate = {2020-06-14},
  abstract = {The social sciences\textemdash including economics\textemdash have long called for transparency in research to counter threats to producing robust and replicable results.  In this paper, we discuss the pros and cons of three of the more prominent proposed approaches: pre-analysis plans, hypothesis registries, and replications.  They have been primarily discussed for experimental research, both in the field including randomized control trials and the laboratory, so we focus on these areas.  A pre-analysis plan is a credibly fixed plan of how a researcher will collect and analyze data, which is submitted before a project begins.  Though pre-analysis plans have been lauded in the popular press and across the social sciences, we will argue that enthusiasm for pre-analysis plans should be tempered for several reasons.  Hypothesis registries are a database of all projects attempted; the goal of this promising mechanism is to alleviate the "file drawer problem," which is that statistically significant results are more likely to be published, while other results are consigned to the researcher's "file drawer."  Finally, we evaluate the efficacy of replications.  We argue that even with modest amounts of researcher bias\textemdash either replication attempts bent on proving or disproving the published work, or poor replication attempts\textemdash replications correct even the most inaccurate beliefs within three to five replications.  We offer practical proposals for how to increase the incentives for researchers to carry out replications.},
  langid = {english},
  keywords = {Cluster Analysis,Factor Models,Multiple or Simultaneous Equation Models: Classification Methods,Principal Components},
  groups = {NAS-Replication-2018},
  timestamp = {2020-06-14T23:58:48Z},
  file = {Coffman_Niederle_2015_Pre-analysis Plans Have Limited Upside, Especially Where Replications Are.pdf:/home/vilhuber/Zotero/storage/TTZXP6AL/Coffman_Niederle_2015_Pre-analysis Plans Have Limited Upside, Especially Where Replications Are.pdf:application/pdf;Snapshot:/home/vilhuber/Zotero/storage/LZZTZ4FP/articles.html:text/html}
}

