  shorttitle = {Transparent {{Social Inquiry}}},
  author = {Elman, Colin and Kapiszewski, Diana and Lupia, Arthur},
  year = {2018},
  month = may,
  journal = {Annual Review of Political Science},
  volume = {21},
  number = {1},
