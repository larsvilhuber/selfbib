  keywords = {Agricultural Extension Services,Agricultural Technology,Belief,Biofuels,Communication,{Diffusion Processes, Agricultural R\&D},Energy,Environment,Information and Knowledge,Learning,Natural Resources,{Other Primary Products, Technological Change: Choices and Consequences},Search,{Unawareness, Economic Development: Agriculture}},
