@article{bradley2021,
  title = {Unrepresentative Big Surveys Significantly Overestimated {{US}} Vaccine Uptake},
  author = {Bradley, Valerie C. and Kuriwaki, Shiro and Isakov, Michael and Sejdinovic, Dino and Meng, Xiao-Li and Flaxman, Seth},
