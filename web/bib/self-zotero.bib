  author = {Laitin, David D. and Miguel, Edward and Alrababa'h, Ala' and Bogdanoski, Aleksandar and Grant, Sean and Hoeberling, Katherine and Mo, Cecilia Hyunjung and Moore, Don A. and Vazire, Simine and Weinstein, Jeremy and Williamson, Scott},
